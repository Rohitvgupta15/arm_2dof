<launch>

<rosparam file="$(find arm_2dof_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="arm_2dof" args="joint 1_position_controller joint2_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/arm_2dof/joint_states"/>
</node>

</launch>